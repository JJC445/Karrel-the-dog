	turnLeft();
}
*/
// Moves down 3, places ball and returns.
function buryBall(){
    turnRight();
    moveTwice();
    move();
    putBall();
    turnAround();
    move();
    moveTwice();
    turnRight();
}
// Places 3 balls
function makePancakes(){
    putBall();
    putBall();
    putBall();
}
// Collect 2 balls
function collectCoins() {
    takeBall();
    takeBall();

}

// Turns around
function turnAround(){
    turnLeft();
    turnLeft();
}


// Move 2 times
function moveTwice(){
    move();
    move();
}

main();
