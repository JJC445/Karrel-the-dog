}

// Moves 3 times
function move3Times(){
    move();
    move();
    move();
}
// Builds a tower
function buildTower(){
    turnLeft();
    putBall();
    move();
    putBall();
    move();
    putBall();
    turnAround();
    moveTwice();
    turnLeft();
}
// Move 4 times.
function move4Times() {
	move();
	move3Times();
}

// Moves over obstacle at front
function jumpHurdle() {
	turnLeft();
	move();
	turnRight();
	move();
	turnRight();
	move();
	turnLeft();
}
/* This function is Obsolete.
