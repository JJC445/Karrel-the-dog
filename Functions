function main() {
    //Insert your code here!
}

/*
*This is a loop, you insert your code and at the second number in the loop is how many
*times you want it to loop or perform a specific task. This is very useful when you want
*to perform a repeated action, such as movement. Here is an example below
*
*This loop will repeat A times before exiting.
    for (let i = 0; i < A; i++) {
	ExampleAction();
	}
 */

// Moves 3 times
function move3Times(){
    move();
    move();
    move();
}
// Builds a tower
function buildTower(){
    turnLeft();
    putBall();
    move();
    putBall();
    move();
    putBall();
    turnAround();
    moveTwice();
    turnLeft();
}
// Move 4 times.
function move4Times() {
	move();
	move3Times();
}

// Moves over obstacle at front
function jumpHurdle() {
	turnLeft();
	move();
	turnRight();
	move();
	turnRight();
	move();
	turnLeft();
}
/* This function is Obsolete.
Turns right
function turnRight() {
	turnLeft();
	turnLeft();
	turnLeft();
}
*/
// Moves down 3, places ball and returns.
function buryBall(){
    turnRight();
    moveTwice();
    move();
    putBall();
    turnAround();
    move();
    moveTwice();
    turnRight();
}
// Places 3 balls
function makePancakes(){
    putBall();
    putBall();
    putBall();
}
// Collect 2 balls
function collectCoins() {
    takeBall();
    takeBall();

}

// Turns around
function turnAround(){
    turnLeft();
    turnLeft();
}


// Move 2 times
function moveTwice(){
    move();
    move();
}

main();
